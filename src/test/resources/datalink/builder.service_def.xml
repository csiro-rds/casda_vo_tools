<?xml version="1.0" encoding="UTF-8" standalone="yes"?><?xml-stylesheet href='http://localhost:8080/casda_vo_tools/votable.xsl' type='text/xsl' ?>

<VOT:VOTABLE xmlns:VOT="http://www.ivoa.net/xml/VOTable/v1.3">
    <VOT:RESOURCE name="CASDA Datalink Result" type="results">
        <VOT:TABLE>
            <VOT:FIELD datatype="char" name="ID" ucd="meta.id;meta.main" arraysize="*"/>
            <VOT:FIELD datatype="char" name="access_url" ucd="meta.ref.url" arraysize="*"/>
            <VOT:FIELD datatype="char" name="service_def" ucd="meta.ref" arraysize="*"/>
            <VOT:FIELD datatype="char" name="error_message" ucd="meta.code.error" arraysize="*"/>
            <VOT:FIELD datatype="char" name="description" ucd="meta.note" arraysize="*"/>
            <VOT:FIELD datatype="char" name="semantics" ucd="meta.code" arraysize="*"/>
            <VOT:FIELD datatype="char" name="content_type" ucd="meta.code.mime" arraysize="*"/>
            <VOT:FIELD unit="byte" datatype="long" name="content_length" ucd="phys.size;meta.file"/>
            <VOT:FIELD ID="authenticatedIdToken" datatype="char" name="authenticated_id_token" ucd="meta.id" arraysize="*"/>
        </VOT:TABLE>
    </VOT:RESOURCE>
    <VOT:RESOURCE type="meta" utype="adhoc:service" ID="async_service">
        <VOT:PARAM name="standardID" datatype="char" arraysize="*" value="ivo://ivoa.net/std/AccessData#async" />
        <VOT:PARAM name="accessURL" datatype="char" arraysize="*" value="http://localhost/data" />
        <VOT:GROUP name="inputParams">
            <VOT:PARAM name="ID" datatype="char" arraysize="*" value="" ref="authenticatedIdToken"/>
        </VOT:GROUP>
    </VOT:RESOURCE>
</VOT:VOTABLE>
